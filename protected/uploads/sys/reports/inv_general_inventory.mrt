        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1"/>
        <Name>inv</Name>
        <NameInSource>localhost</NameInSource>
        <Parameters count="0" isList="true"/>
        <SqlCommand>SELECT 
product_id,
`code`,
product_name,
uom_name,
category_name,
warehouse_name,
sum(preQty)preQty ,
sum(preAmt) preAmt,
sum(CurrInQty) CurrInQty,
sum(currInAmt) currInAmt,
sum(CurrOutQty) CurrOutQty,
sum(currOutAmt) currOutAmt
 FROM (
SELECT
d.product_id,
p.`code`,
p.`name` product_name,
u.`name` uom_name,
c.`name` category_name,
w.`name` warehouse_name,
d.qty preQty,
d.qty*d.cost preAmt,
0 CurrInQty,
0 currInAmt,
0 CurrOutQty,
0  currOutAmt
FROM
inv_inventorylog i
INNER JOIN inv_inventorylogdetail d ON i.id = d.inventorylog_id
INNER JOIN product_item p ON p.id  = d.product_id
INNER JOIN product_category c ON c.id  = p.category_id
INNER JOIN inv_warehouse w ON w.id  = d.wh_id
INNER JOIN product_uom u ON u.id  = d.uom_id

WHERE i.refdate &lt; STR_TO_DATE('{fdate}','%d-%m-%Y') AND  d.wh_id=COALESCE(case {whid} when 0 then null else {whid} end,d.wh_id) 
			And p.category_id=COALESCE(case {category_id} when 0 then null else {category_id} end,p.category_id)  And p.type_id=COALESCE(case '{type_id}' when '' then null else '{type_id}' end,p.type_id)
UNION All

SELECT
d.product_id,
p.`code`,
p.`name` product_name,
u.`name` uom_name,
c.`name` category_name,
w.`name` warehouse_name,
0,
0,
CASE WHEN  d.qty&gt;0 THEN d.qty ELSE 0 END as CurrInQty,
CASE WHEN  d.qty&gt;0 THEN d.qty*d.cost ELSE 0 END  as currInAmt,
CASE WHEN  d.qty&lt;0 THEN -d.qty ELSE 0 END as CurrOutQty,
CASE WHEN  d.qty&lt;0 THEN -d.qty*d.cost ELSE 0 END  as currOutAmt
FROM
inv_inventorylog i
INNER JOIN inv_inventorylogdetail d ON i.id = d.inventorylog_id
INNER JOIN product_item p ON p.id  = d.product_id
INNER JOIN product_category c ON c.id  = p.category_id
INNER JOIN inv_warehouse w ON w.id  = d.wh_id
INNER JOIN product_uom u ON u.id  = d.uom_id
WHERE i.refdate &gt;= STR_TO_DATE('{fdate}','%d-%m-%Y') AND i.refdate &lt;= STR_TO_DATE('{tdate}','%d-%m-%Y') 
AND i.reftype&gt;0 
AND  d.wh_id=COALESCE(case {whid} when 0 then null else {whid} end,d.wh_id) 
And p.category_id=COALESCE(case {category_id} when 0 then null else {category_id} end,p.category_id)  
And p.type_id=COALESCE(case '{type_id}' when '' then null else '{type_id}' end ,p.type_id)
) a GROUP BY product_id,code,product_name,uom_name,category_name,warehouse_name</SqlCommand>
      </inv>
